   <%= stylesheet_link_tag "application.css" %>
<div class ="login">
    <h1>Log In</h1>
    <%=form_tag log_in_path do%>
    <%=label_tag :email %>
    <%=text_field_tag :email %>
    <%=label_tag :password %>
    <%=password_field_tag :password %>
  <label class="remember">
  <input id="PersistentCookie" name="PersistentCookie"
          type="checkbox" value="yes"
          >
    
    <%=submit_tag "Log in" %>
    <%end%>
</div>
